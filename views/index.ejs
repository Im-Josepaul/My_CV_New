  <!-- ======= Resume Section ======= -->
  <section id="resume" class="resume">
    <div class="container">

      <div class="section-title">
        <h2>Resume</h2>
        <p>Check My Resume</p>
      </div>

      <div class="row">
        <div class="col-lg-6">
          <div class="resume-item pb-0">
            <h4>Summary</h4>
            <p><em>
              <%= resume.summary.summary_text %>
            </em></p>
            <p>
            <ul>
              <li><%= resume.summary.point1%></li>
              <li><%= resume.summary.point2%></li>
              <li><%= resume.summary.point3%></li>
            </ul>
            </p>
          </div>

          <h3 class="resume-title">Education</h3>

          <% for (const [course,details] of Object.entries(resume.education)){ %>
            <div class="resume-item">
              <h4><%= details.course_name%></h4>
              <h5><%= details.startyear%> - <%= details.endyear || ' '%></h5>
              <p><em><%= details.location%></em></p>
              <p><%= details.info%></p>
            </div>
          <% } %>  
        </div>
        <div class="col-lg-6">
          <h3 class="resume-title">Professional Experience</h3>
          <% for(const [key,value] of Object.entries(resume.experience)){ %>
            <div class="resume-item">
              <h4><%= value.role%></h4>
              <h5><%= value.year%></h5>
              <p><em><%= value.company%></em></p>
              <p>
                <ul>
                  <% for(const [id,data] of Object.entries(value.info)){ %>
                    <li><%= data %></li>
                  <% } %>
                </ul>
              </p>
            </div>
          <% } %>
          
          <h3 class="resume-title">Achievements &amp; Responsibilities</h3>
          <% for(const [key,value] of Object.entries(resume.achievements)){ %>
            <div class="resume-item">
              <h4><%= value.title%></h4>
              <h5><%= value.from%> 
                <% if(value.to) { %>
                  - <%=value.to%>
                <% } %>
              </h5>
              <p><em><%= value.Location%></em></p>
              <p>
                <ul>
                  <% for(const [id,data] of Object.entries(value.info)){ %>
                    <li><%= data%> </li>
                  <% } %>
                </ul>
              </p>
            </div>
          <% } %>
        </div>
      </div>

    </div>
  </section>
  <!-- End Resume Section -->

